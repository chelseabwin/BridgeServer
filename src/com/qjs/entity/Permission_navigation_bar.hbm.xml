        </id>
        <property name="permission_id" type="java.lang.Integer">
            <column name="permission_id" not-null="true" />
        </property>
        <property name="navigation_bar_id" type="java.lang.Integer">
            <column name="navigation_bar_id" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
