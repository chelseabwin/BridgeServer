<hibernate-mapping>
    <class name="com.qjs.entity.Permission_navigation_bar" table="permission_navigation_bar" catalog="bridge_server">
        <id name="permission_navigation_bar_id" type="java.lang.Integer">
            <column name="permission_navigation_bar_id" />
