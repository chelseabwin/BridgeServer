<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.qjs.entity.Parts1" table="parts1" catalog="bridge_server">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native"></generator>
        </id>
        <property name="bg_id" type="java.lang.String">
             <column name="bg_id" length="200" />
        </property>
        <property name="wing_wall" type="java.lang.String">
             <column name="wing_wall" length="200" />
        </property>
        <property name="conical_slope" type="java.lang.String">
             <column name="conical_slope" length="200" />
        </property>
        <property name="protection_slope" type="java.lang.String">
             <column name="protection_slope" length="200" />
        </property>
        <property name="pier_detail" type="java.lang.String">
             <column name="pier_detail" length="200" />
        </property>
        <property name="abutment_num" type="java.lang.String">
             <column name="abutment_num" length="200" />
        </property>
        <property name="pa_num" type="java.lang.String">
             <column name="pa_num" length="200" />
        </property>
        <property name="bed_num" type="java.lang.String">
             <column name="bed_num" length="200" />
        </property>
        <property name="reg_structure" type="java.lang.String">
             <column name="reg_structure" length="200" />
        </property>
        <property name="flag" type="java.lang.String">
             <column name="flag" length="10" />
        </property>
    </class>
</hibernate-mapping>
